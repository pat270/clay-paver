	{ id: 'tooltip-max-width', },
	{ id: 'tooltip-font-size', },
	{ id: 'tooltip-padding-y', },
	{ id: 'tooltip-padding-x', },
	{ id: 'tooltip-margin', },
	{ id: 'tooltip-border-radius', },
	{ id: 'tooltip-color', color: true, },
	{ id: 'tooltip-bg', color: true, },
