<h4 class="cp-variables-form-header">Nav</h4>
<%-
	printInputs([
		{ id: 'nav-font-size', },
	]);
%>

<h4 class="cp-variables-form-header">Nav Form</h4>
<%-
	printInputs([
		{ id: 'nav-form-padding-top', },
		{ id: 'nav-form-padding-right', },
		{ id: 'nav-form-padding-bottom', },
		{ id: 'nav-form-padding-left', },
	]);
%>

