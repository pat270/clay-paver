<h4 class="cp-variables-form-header">Label</h4>
<%-
	printInputs([
		{ id: 'label-height', },
		{ id: 'label-font-size', },
		{ id: 'label-font-weight', },
		{ id: 'label-line-height', },
		{ id: 'label-padding-y', },
		{ id: 'label-padding-x', },
		{ id: 'label-spacer-y', },
		{ id: 'label-spacer-x', },
		{ id: 'label-border-width', },
		{ id: 'label-border-style', },
		{ id: 'label-border-color', color: true, },
		{ id: 'label-border-radius', },
		{ id: 'label-color', color: true, },
		{ id: 'label-text-transform', },
	]);
%>

