<script>
	function cpTypographyPopulateItems() {
		$('.cp-populate-content').each(function(index) {
			var fontFamily = $(this).css('content').replace(/"/g, "");

			$(this).text(fontFamily);
		});
	}

	$(document).on('cp-compile-done', function(event) {
		cpTypographyPopulateItems();
	});

	$(document).on('turbolinks:load', function(event) {
		cpTypographyPopulateItems();
	});
</script>